#!/usr/bin/env bash

if [[ -z ${1} ]]; then
	echo "usage: $0 str"
	exit 1
fi

. _PREFIX_/share/kde-l10n-scripts/common.sh

pushd "${KDEREPO_PATH}/trunk/l10n-support/${KDE_LANG}/summit" > /dev/null || exit 1

echo "Seaching for string \"${1}\""
${POSIEVE} find-messages -s "msgid:${1}" -s lokalize messages > /dev/null

popd > /dev/null
