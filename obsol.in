#!/usr/bin/env bash

. _PREFIX_/share/kde-l10n-scripts/common.sh

pushd "${KDEREPO_PATH}/trunk" > /dev/null || exit 1

echo "Updating repositories at \"${KDEREPO_PATH}/trunk\""
svn up || exit 1

popd > /dev/null

pushd "${KDEREPO_PATH}/trunk/l10n-support/cs/summit" > /dev/null || exit 1

echo "Seaching for obsolete strings"
${POSIEVE} find-messages -s obsol -s lokalize messages > /dev/null

popd > /dev/null
